    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:regression": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:e2e:report": "playwright show-report",
    "test:runner": "node scripts/run-tests.js",
    "test:all": "node scripts/run-all-tests.js",
    "test:all:e2e": "node scripts/run-all-tests.js --e2e",
